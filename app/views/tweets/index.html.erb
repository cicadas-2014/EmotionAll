<h1>Listing tweets</h1>

<table>
  <thead>
    <tr>
      <th>Tweet</th>
      <th>Tweetid</th>
      <th>Retweet count</th>
      <th>Language</th>
      <th>Country code</th>
      <th>Longitude</th>
      <th>Latitude</th>
      <th>Sentiment</th>
      <th>Sentiment score</th>
      <th>Trend</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tweets.each do |tweet| %>
      <tr>
        <td><%= tweet.tweet %></td>
        <td><%= tweet.tweetid %></td>
        <td><%= tweet.retweet_count %></td>
        <td><%= tweet.language %></td>
        <td><%= tweet.country_code %></td>
        <td><%= tweet.longitude %></td>
        <td><%= tweet.latitude %></td>
        <td><%= tweet.sentiment %></td>
        <td><%= tweet.sentiment_score %></td>
        <td><%= tweet.trend_id %></td>
        <td><%= link_to 'Show', tweet %></td>
        <td><%= link_to 'Edit', edit_tweet_path(tweet) %></td>
        <td><%= link_to 'Destroy', tweet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tweet', new_tweet_path %>
